                LOOP=1 CONCURRENCY_LIST="1 32" ../common_tools/benchmark-http.sh https://127.0.0.1:8443
            '''
        }
        */

    try {    
        timeout(time: 5, unit: 'MINUTES') {
            sh '''
                cd Examples/blender
                make ${MAKEOPTS} all
                make check
            '''
        }
    } catch (Exception e){
        build_ok = false
        sh 'echo "Blender Example Test Failed"'
    }

    try {
        timeout(time: 5, unit: 'MINUTES') {
            sh '''
                cd Examples/r
                make ${MAKEOPTS} all
                make check
            '''
        }
    } catch (Exception e){
        build_ok = false
        sh 'echo "R Example Test Failed"'
    }
