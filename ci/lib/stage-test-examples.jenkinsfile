stage('test-examples') {

    if (env.SGX == "1") {
        env.MODE = "sgx"
    }
    else {
        env.MODE = "direct"
    }

    if (["ubuntu", "debian"].contains(env.os_release_id)) {
            try {
                  sh '''
			cd CI-Examples/mysql
			sudo -E make install-dependencies && make privileges
			make ${MAKEOPTS} all && make ${MAKEOPTS} check
			sleep 60s
                        make conncheck
			sudo make uninstall-mysql
                    '''
         } catch (Exception e) { }
     }
}
