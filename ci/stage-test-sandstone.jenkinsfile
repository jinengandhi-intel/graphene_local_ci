stage('test') {
    env.no_cpu = sh(script:'nproc', returnStdout: true).trim()
    timeout(time: 90, unit: 'MINUTES') {
        sh '''
            if [ "${no_cpu}" -gt 16 ]
            then
                cd CI-Examples/sandstone-50-bin
                if test -n "$SGX"
                then
                    make SGX=1 
                    SGX=1 make start-sandstone 2>&1 | tee OUTPUT.txt
                fi
            fi
        '''
    }
}
            
